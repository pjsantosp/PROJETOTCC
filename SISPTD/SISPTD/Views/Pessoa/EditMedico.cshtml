
@{
    ViewBag.Title = "Editar Medico";
}

@model SISPTD.Models.Pessoa
<h4><i class="fa fa-user-md fa-lg" aria-hidden="true"></i> Editar Médico</h4>
<span class="text-danger pull-right"><i class="fa fa-info-circle "></i> Os campos com o marcador (*)  são de preenchimento obrigatório.</span>
<br />

@using (Html.BeginForm())
{
    @Html.HiddenFor(p => p.pessoaId)
    @Html.Partial("_FormMedico", Model)

}

@section Scripts {
    @Scripts.Render("~/bundles/jqueryval")
}





