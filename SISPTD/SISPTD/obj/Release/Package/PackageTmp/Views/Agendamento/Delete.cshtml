@model SISPTD.Models.Agendamento
@{
    ViewBag.Title = "Delete";
}
<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div class="panel panel-default">
            <div class="">
                <h3 class="text-center"> <i class="fa fa-question-circle" aria-hidden="true"></i> Tem Certeza que Deseja Excluir o Agendamento ?</h3>
                <hr />
            </div>
            <div class="panel-body">
                <h4>
                <i class="fa fa-th-large" aria-hidden="true"></i> 
                    @Html.DisplayNameFor(model => model.Processo.Paciente.nome)
                        @Html.DisplayFor(model => model.Processo.Paciente.nome)
                        @Html.DisplayNameFor(model => model.dt_Agendamento)

                        @Html.DisplayFor(model => model.dt_Agendamento)
                 </h4>
                <hr />

                @using (Html.BeginForm())
                {
                    @Html.AntiForgeryToken()

                    <div class="form-actions no-color">
                        <input type="submit" value="Sim" class="btn btn-danger" /> |
                        <a href="@Url.Action("Index","Setor")" class="btn btn-primary">Não</a>
                    </div>
                }
            </div>
        </div>

    </div>
</div>






