            <tr>
                <th>
                    @Html.LabelFor(model => model.Paciente.RequisicaoComoAcompanhante, new { @class = "control-label" })
                </th>
                <th>
                    @Html.LabelFor(model => model.PessoaAcompanhante, new { @class = "control-label" })
                </th>
