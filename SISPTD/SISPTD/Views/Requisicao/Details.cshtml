                </th>
                <th>
                    @Html.LabelFor(model => model.PessoaAcompanhante, new { @class = "control-label" })
                </th>
            </tr>
            @foreach (var item in Model.Paciente.RequisicaoComoAcompanhante)
            {
                <tr>
                    <td>
                        @item.Paciente.RequisicaoComoAcompanhante
                    </td>
                    <td>
                        @if (User.IsInRole("Medico"))
                        {
                            @Html.Raw("<p>" + item.nome + "</p>");
                        }
                        else
                        {
                            <a href="@Url.Action("Edit", "Pessoa", new { id = item.pessoaId })">@item.nome</a>
                        }
                    </td>
                    <td>
                        @item.rg
                    </td>
                    <td>
                        @item.orgaoemissor
                    </td>
                    <td>
                        @item.dt_Emissao
                    </td>
                    <td>
                        @item.cns
                    </td>
                </tr>
            }
        </table>
