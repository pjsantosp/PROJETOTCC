@model IEnumerable<SISPTD.Models.Requisicao>


<div class="form-horizontal">
    <h4>Itens da Requisição Acompanhantes</h4>
    <div class="row">
        <div class="col-md-3">
            <div class="form-group">
                @Html.TextBox("buscar", Request.QueryString["CPF"], new { autofocus = "", @class = " procurarPessoa col-md-2 form-control", placeholder = "Procure por CPF" })
            </div>

        </div>
        <div class="col-md-7">
            <div class="form-group">
                <input type="text" id="paciente" readonly class="form-control paciente" name="name" value=" " />
                @* @Html.EditorFor(model => model.Pessoa.nome, new { HtmlAttributes = new {disabled="disabled",  @class = "form-control paciente readonly" } })*@
            </div>

        </div>

    </div>
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("Data")
        </th>
        <th>
            @Html.DisplayName("CPF")
        </th> 
        <th>
            @Html.DisplayName("Nome")
        </th>
        <th>
            @Html.DisplayName("Classificação")
        </th>
        <th>
            @Html.DisplayName("Observação")
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.observacoes)
            </td>
         
            <td>
             
            </td>
        </tr>
    }

</table>
